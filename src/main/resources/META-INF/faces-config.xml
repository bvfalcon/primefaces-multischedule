<?xml version="1.0" encoding="utf-8"?>
<faces-config version="2.0" xmlns="http://java.sun.com/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">

    <name>bychkov_primefaces</name>

    <component>
        <component-type>name.bychkov.primefaces.component.multischedule.MultiSchedule</component-type>
        <component-class>name.bychkov.primefaces.component.multischedule.MultiSchedule</component-class>
    </component>

    <render-kit>
        <renderer>
            <component-family>org.primefaces.component</component-family>
            <renderer-type>name.bychkov.primefaces.component.multischedule.MultiSchedule</renderer-type>
            <renderer-class>name.bychkov.primefaces.component.multischedule.MultiScheduleRenderer</renderer-class>
        </renderer>
        <client-behavior-renderer>
            <client-behavior-renderer-type>org.primefaces.component.AjaxBehaviorRenderer</client-behavior-renderer-type>
            <client-behavior-renderer-class>org.primefaces.behavior.ajax.AjaxBehaviorRenderer</client-behavior-renderer-class>
        </client-behavior-renderer>
    </render-kit>

</faces-config>